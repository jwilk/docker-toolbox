#!/bin/sh

# Copyright Â© 2022 Jakub Wilk <jwilk@jwilk.net>
# SPDX-License-Identifier: MIT

set -e -u

usage() {
    printf 'Usage: %s CNAME\n' "${0##*/}"
}
if [ $# -ne 1 ]
then
    usage >&2
    exit 1
fi
cname="$1"
docker manifest inspect "$cname" | jq -r '.manifests[] | .platform.os + "/" + .platform.architecture'

# vim:ts=4 sts=4 sw=4 et
